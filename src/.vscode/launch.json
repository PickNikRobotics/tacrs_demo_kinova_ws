{
    // For more information, go to: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Attach",
            "type": "cppdbg",
            "request": "attach",
            "program": "/opt/overlay_ws/install/moveit_studio_agent/lib/moveit_studio_agent/objective_server_node_main",
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "ROS: Launch",
            "type": "ros",
            "request": "launch",
            "arguments": ["hw_env:=hw_devices", "launch_rviz:=false"],
            "target": "/opt/overlay_ws/install/schilling_rovs_bringup/share/schilling_rovs_bringup/launch/rov.launch.py"
        },
        {
            "name": "Debug",
            "type": "cppdbg",
            "request": "launch",
            // "preLaunchTask": "Full debug build",
            "cwd": "${workspaceFolder}",
            "program": "${workspaceFolder}/../build/example",
            "stopAtEntry": false,
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "pretty-print for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "miDebuggerPath": "/usr/bin/gdb",
            "envFile": "${workspaceFolder}/.vscode/.env"
        },
        {
            "name": "Attach to ros launch",
            "request": "launch",
            "type": "cppdbg",
            "miDebuggerServerAddress": "localhost:3000",
            "cwd": "${workspaceFolder}",
            "program": "~/user_ws/install/fuse_optimizers/lib/fuse_optimizers/fixed_lag_smoother_node",
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "pretty-print for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "miDebuggerPath": "/usr/bin/gdb"
        }
    ]
}